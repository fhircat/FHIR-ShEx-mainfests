PREFIX fhir: <http://hl7.org/fhir/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<cs1> a fhir:CapabilityStatement ;
  fhir:extension [ fhir:url <miscUrl> ; fhir:value [ fhir:v "launch-standalone" ] ] ;
  fhir:status [ fhir:v "active"^^xsd:token ] ;
  fhir:rest [
    fhir:extension [ fhir:url <miscUrl> ; fhir:value [ fhir:v "launch-standalone" ] ] ;
    fhir:security [
      fhir:extension [ fhir:url <miscUrl> ; fhir:value [ fhir:v "launch-standalone" ] ] ;
      # <capExt> is only allowed here
      fhir:extension [ fhir:url <capUrl> ; fhir:value [ fhir:v "launch-standalone" ] ] ;
      fhir:cors [ fhir:v true ]
    ]
  ] .

<capUrl> fhir:v "http://fhir-registry.smarthealthit.org/StructureDefinition/capabilities"^^xsd:anyURI .
<miscUrl> fhir:v "http://other.example/url"^^xsd:anyURI .