PREFIX fhir: <http://hl7.org/fhir/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

<cs1> a fhir:CapabilityStatement ;
  fhir:extension _:miscExt ;
  fhir:status [ fhir:v "active"^^xsd:token ] ;
  fhir:rest [
    fhir:extension _:miscExt ;
    fhir:security [
      fhir:extension _:miscExt ;
      fhir:extension _:capExt ; # _:capExt is only allowed here
      fhir:cors [ fhir:v true ]
    ]
  ] .

_:capExt fhir:url [ fhir:v "http://fhir-registry.smarthealthit.org/StructureDefinition/capabilities"^^xsd:anyURI ] ;
  fhir:value [ fhir:v "launch-standalone" ] .

_:miscExt fhir:url [ fhir:v "http://other.example/url"^^xsd:anyURI ] ;
  fhir:value [ fhir:v true ] .
